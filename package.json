{
	"name": "farmer",
	"private": true,
	"version": "0.1.6",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "tauri",
		"tauri:dev": "tauri dev",
		"tauri:dev:vendored": "tauri dev -f vendored-openssl",
		"build:linux": "bun run build && bun tauri build -f vendored-openssl",
		"build:mac": "bun run build && bun tauri build --target universal-apple-darwin -f vendored-openssl",
		"build:windows": "bun run build && bun tauri build --target x86_64-pc-windows-msvc -f vendored-openssl",
		"typecheck": "tsc --noEmit",
		"lint": "bunx --bun @biomejs/biome check src/",
		"lint:fix": "bunx --bun @biomejs/biome check --write src/"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.4",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/react-select": "^2.1.4",
		"@tauri-apps/api": "^2",
		"@tauri-apps/plugin-clipboard-manager": "^2.3.2",
		"@tauri-apps/plugin-dialog": "^2.6.0",
		"@tauri-apps/plugin-opener": "^2",
		"@tauri-apps/plugin-shell": "^2.3.5",
		"@tauri-apps/plugin-updater": "^2",
		"@tauri-apps/plugin-process": "^2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.563.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"zustand": "^5.0.2"
	},
	"devDependencies": {
		"@profi.co/biome-config": "^1.0.1",
		"@tauri-apps/cli": "^2",
		"@types/node": "^25.2.0",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@typescript-eslint/eslint-plugin": "^8.19.1",
		"@typescript-eslint/parser": "^8.19.1",
		"@vitejs/plugin-react": "^4.6.0",
		"autoprefixer": "^10.4.24",
		"eslint": "^9",
		"eslint-plugin-react-hooks": "^5.1.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^3",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.19.1",
		"vite": "^7.0.4"
	}
}
